    "def f(arr):\n",
    "    n = len(arr)\n",
    "    i = 0\n",
    "    j = n - 1\n",
    "    while i < j:\n",
    "        s = arr[i] + arr[j]\n",
    "        if s == target:\n",
    "            return [i,j]\n",
    "        elif s>target:\n",
    "            j -= 1\n",
    "        else:\n",
    "            i += 1\n",
    "    return -1\n",
    "arr = [2,6,7,15,25,30]\n",
    "target = 31\n",
    "f(arr)"
   ]
